from tkinter import *

root = Tk()
root.geometry('500x500')

def calculate():
    newVar = station1

def changeResult():
    print("rawr")
    newtext = "Updated"
    labelResult.config(text=newtext)

    

topFrame=Frame(root)
station1 = Text(root, height=1, width=30)
station1.insert(END, "Just a text Widget")

buttonCheck = Button(topFrame, text='Check Market', command=changeResult)
labelResult = Label(topFrame, text='DEFAULTVALUE')

topFrame.pack()

buttonCheck.pack(side=TOP)
station1.pack(side=TOP)
labelResult.pack(side=TOP)
root.mainloop()
